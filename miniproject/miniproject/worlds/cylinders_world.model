          <geometry>
            <cylinder>
              <radius>0.2</radius>
              <length>0.5</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>1 1 1 1</ambient>
          </material>
        </visual>
      </link>
      <pose frame=''>1 1 0 0 0 0</pose>
      <static>true</static>
    </model>

    <model name="cylinder3">
      <link name="link">
        <collision name="collision">
          <pose frame=''>0 0 0.25 0 -0 0</pose>
